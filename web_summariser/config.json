{
  "models": {
    "openai": {
      "provider": "openai",
      "model": "gpt-5-nano"
    },
    "ollama": {
      "provider": "ollama",
      "model": "deepseek-r1:8b",
      "api_base": "http://localhost:11434/api/chat"
    }
  },
  "default_provider": "openai",
  "scraper": {
    "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/117.0.0.0 Safari/537.36",
    "timeout": 30
  },
  "output": {
    "directory": "output"
  }
}
